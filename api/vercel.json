{
  "version": 2,
  "buildCommand": "nest build",
  "installCommand": "pnpm install --force",
  "framework": null,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}